import com.data.Employee;

dialect  "mvel"

rule "Suggest Manager Role"
    when
        e:Employee(experience >= 10 && salary > 100 && salary <= 2500000)
    then
        e.setRole("Manager");
end

rule "Suggest current Role"
    when
        e:Employee(salary < 10)
    then
        e.setRole("Clerk");
end